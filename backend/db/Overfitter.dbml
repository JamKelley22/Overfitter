Table outfit {
  id int [primary key]
  uriImage text
  occasion outfit_type
  topId int [ref: > tops.id]
  bottomId int [ref: > bottoms.id]
  headId int [ref: > head.id]
  feetId int [ref: > feet.id]
  wristId int [ref: > wrist.id]
  rating int
}

enum outfit_type {
    FORMAL
    CASUAL
    BUSINESS
    BUSINESS_CASUAL
    SPORT
    SLEEP
}

enum top_type {
    TEE
    SWEAT
    DRESS
    FLANNEL
    UNDER
    POLO
    SLEEP
    CASUAL
    WORKOUT
    UNKNOWN
}

enum bottom_type {
    SWEAT
    DRESS
    UNDER
    SLEEP
    WORKOUT
    JEANS
    KAKIS
    UNKNOWN
}

enum bottom_inseam_type {
  SHORT
  REGULAR
  TALL
}

enum feet_type {
    DRESS_SHOE
    TENNIS_SHOE
    CASUAL_SHOE
    HEEL_SHOE
    FLIP_FLOP_SHOE
    SANDAL_SHOE
    DRESS_SOCK
    CASUAL_SOCK
    UNKNOWN
}

enum wrist_type {
    WATCH
    BRACLET
    UNKNOWN
}

enum head_type {
    BASEBALL_HAT
    BEANIE
    BANDANA
    GLASSES
    UNKNOWN
}

enum item_status {
    CLEAN
    WORN
    DIRTY
    UNKNOWN
}

enum item_condition {
    NEW
    LIKE_NEW
    GOOD
    FAIR
    POOR
    TRASH
    UNKNOWN
}

enum size_us_letter {
    5XS
    4XS
    3XS
    2XS
    XS
    S
    M
    L
    XL
    2XL
    3XL
    4XL
    5XL
    UNKNOWN
}